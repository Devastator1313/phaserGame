{
  "name": "phaser-game",
  "version": "1.1.2",
  "description": "Phaser game originally made for a research project in my Advanced Web and Internet Programming class",
  "scripts": {
    "audio": "audiosprite --loop freeVertexStudioTrack1 --loop freeVertexStudioTrack2 --output public/assets/audio/gameAudioSprite raw_assets/audio/*.mp3",
    "devTutorial": "esbuild src/mainTutorial.js --serve=3000 --servedir=public --bundle --sourcemap --target=es6 --define:__DEV__=true --outdir=public/",
    "prodTutorial": "esbuild src/mainTutorial.js --serve=3000 --servedir=public --bundle --minify --target=es6 --define:__DEV__=false --outdir=public/",
    "buildTutorial": "esbuild src/mainTutorial.js --bundle --minify --target=es6 --define:__DEV__=false --outdir=public/",
    "dev": "esbuild src/main.js --serve=3000 --servedir=public --bundle --sourcemap --target=es6 --define:__DEV__=true --outdir=public/",
    "prod": "esbuild src/main.js --serve=3000 --servedir=public --bundle --minify --target=es6 --define:__DEV__=false --outdir=public/",
    "build": "esbuild src/main.js --bundle --minify --target=es6 --define:__DEV__=false --outdir=public/",
    "preinstall": "npx npm-force-resolutions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devastator1313/phaserGame.git"
  },
  "bugs": {
    "url": "https://github.com/Devastator1313/phaserGame/issues"
  },
  "author": "Devan Olson",
  "license": "MIT",
  "homepage": "https://github.com/Devastator1313/phaserGame#readme",
  "resolutions": {
    "minimist": "1.2.6",
    "underscore": "1.12.1"
  },
  "devDependencies": {
    "audiosprite": "^0.2.1",
    "debounce": "^2.2.0",
    "esbuild": "^0.25.4",
    "eslint": "^8.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "phaser": "^3.87.0"
  }
}
